@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.org/sepio-model/Document> a sh:NodeShape ;
    rdfs:subClassOf <https://example.org/sepio-model/InformationEntity> ;
    sh:closed true ;
    sh:description "A collection of information, usually in a text-based or graphic human-readable form, intended to be read and understood together as a whole." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Document" ;
    sh:property [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "Types of methodological approaches that were executed to directly or indirectly support creation of the Information Entity." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 11 ;
            sh:path <https://example.org/sepio-model/supportingMethodTypes> ],
        [ sh:class <https://example.org/sepio-model/Extension> ;
            sh:description "A list of extensions to the Entity, that allow for capture of information not directly supported by elements defined in the model." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 22 ;
            sh:path <https://example.org/sepio-model/extensions> ],
        [ sh:datatype xsd:string ;
            sh:description "A specification that describes all or part of the process that led to creation of the Information Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path <https://example.org/sepio-model/specifiedBy> ],
        [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "A specific type of document that a Document instance represents (e.g.  'publication', 'patent', 'pathology report')" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <https://example.org/sepio-model/subtype> ],
        [ sh:class <https://example.org/sepio-model/InformationEntity> ;
            sh:description "Another Information Entity from which this Information Entity is derived, in whole or in part." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 12 ;
            sh:path <https://example.org/sepio-model/derivedFrom> ],
        [ sh:datatype xsd:string ;
            sh:description "The 'logical' identifier of the entity in the system of record, e.g. a UUID.  This 'id' is unique within a given system, but may or may not be globally unique outside the system. It is used within a system to reference one object from another." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path <https://example.org/sepio-model/id> ],
        [ sh:datatype xsd:string ;
            sh:description "An entity or concept in the world that the information entity describes/is about." ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <https://example.org/sepio-model/isAbout> ],
        [ sh:datatype xsd:string ;
            sh:description "A `PubMed unique identifier <https://en.wikipedia.org/wiki/PubMed#PubMed_identifier>`_ for the document." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <https://example.org/sepio-model/pmid> ],
        [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "A qualitative term indicating the scientific rigor or reliability with which the information was generated/collected." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 14 ;
            sh:path <https://example.org/sepio-model/informationQuality> ],
        [ sh:class <https://example.org/sepio-model/RecordMetadata> ;
            sh:description "Provenance metadata about a specific concrete record of information as encoded/serialized in a particular data set or object (as opposed to provenance about the abstract information content the encoding carries)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 15 ;
            sh:path <https://example.org/sepio-model/recordMetadata> ],
        [ sh:datatype xsd:string ;
            sh:description "A `Digital Object Identifier <https://www.doi.org/the-identifier/what-is-a-doi/>_` for the document." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path <https://example.org/sepio-model/doi> ],
        [ sh:datatype xsd:string ;
            sh:description "The official title given to the document by its authors." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://example.org/sepio-model/title> ],
        [ sh:datatype xsd:string ;
            sh:description "A specific license that dictates legal permissions for how a document can be used (by whom, where, for what purposes, with what additional requirements, etc.)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path <https://example.org/sepio-model/license> ],
        [ sh:class <https://example.org/sepio-model/Contribution> ;
            sh:description "Specific actions taken by an Agent toward the creation, modification, validation, or deprecation of an Information Entity." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 7 ;
            sh:path <https://example.org/sepio-model/contributions> ],
        [ sh:datatype xsd:string ;
            sh:description "One or more URLs from which the content of the Document can be retrieved." ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://example.org/sepio-model/urls> ],
        [ sh:datatype xsd:string ;
            sh:description "The name of the class that is instantiated by a data object representing the Entity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 18 ;
            sh:path <https://example.org/sepio-model/type> ],
        [ sh:datatype xsd:string ;
            sh:description "Specific methods that were executed to directly or indirectly support creation of the Information Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path <https://example.org/sepio-model/supportingMethods> ],
        [ sh:datatype xsd:string ;
            sh:description "A document in which the Information Entity is reported." ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path <https://example.org/sepio-model/reportedIn> ],
        [ sh:datatype xsd:string ;
            sh:description "A globally-unique 'business' identifier or accession number for the real-world entity represented by a data object. These are typically assigned by an external system or authority, and used to connect entities and share content across different systems." ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path <https://example.org/sepio-model/identifiers> ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates when the information content expressed in the Information Entity was generated." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path <https://example.org/sepio-model/dateAuthored> ],
        [ sh:datatype xsd:string ;
            sh:description "A free text description of the Entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 21 ;
            sh:path <https://example.org/sepio-model/description> ],
        [ sh:datatype xsd:string ;
            sh:description "Alternative name(s) for the Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 20 ;
            sh:path <https://example.org/sepio-model/alternativeLabels> ],
        [ sh:datatype xsd:string ;
            sh:description "A primary name for the Entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 19 ;
            sh:path <https://example.org/sepio-model/label> ] ;
    sh:targetClass <https://example.org/sepio-model/Document> .

<https://example.org/sepio-model/Expression> a sh:NodeShape ;
    rdfs:subClassOf <https://example.org/sepio-model/Utility> ;
    sh:closed true ;
    sh:description "A structure for labels representing systematic expressions that describe an entity, as generated by formal nomenclatures (e.g. HGVS for genetic variants, ISCN for karyotypes, HLA nomenclature for HLA genes/alleles)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Expression" ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "A free-text rendering of the expression used as a label for the entity" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://example.org/sepio-model/value> ],
        [ sh:datatype xsd:string ;
            sh:description "The version of the nomenclature system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://example.org/sepio-model/systemVersion> ],
        [ sh:datatype xsd:anyURI ;
            sh:description "A URL for the nomenclature system" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://example.org/sepio-model/systemURL> ] ;
    sh:targetClass <https://example.org/sepio-model/Expression> .

<https://example.org/sepio-model/Statement> a sh:NodeShape ;
    rdfs:subClassOf <https://example.org/sepio-model/InformationEntity> ;
    sh:closed true ;
    sh:description "A claim of purported truth as made by a particular agent, on a particular occasion. Statements may be used to simply put forth a possible fact (i.e. a 'proposition') as true, or to provide a more nuanced assessment of the level of confidence or evidence supporting a particular proposition." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Statement" ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The name of the class that is instantiated by a data object representing the Entity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 25 ;
            sh:path <https://example.org/sepio-model/type> ],
        [ sh:class <https://example.org/sepio-model/Contribution> ;
            sh:description "Specific actions taken by an Agent toward the creation, modification, validation, or deprecation of an Information Entity." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 14 ;
            sh:path <https://example.org/sepio-model/contributions> ],
        [ sh:datatype xsd:string ;
            sh:description "A free text description of the Entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 28 ;
            sh:path <https://example.org/sepio-model/description> ],
        [ sh:class <https://example.org/sepio-model/InformationEntity> ;
            sh:description "Another Information Entity from which this Information Entity is derived, in whole or in part." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 19 ;
            sh:path <https://example.org/sepio-model/derivedFrom> ],
        [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "A qualitative term indicating the scientific rigor or reliability with which the information was generated/collected." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 21 ;
            sh:path <https://example.org/sepio-model/informationQuality> ],
        [ sh:datatype xsd:string ;
            sh:description "A term indicating whether the Statement supports, disputes, or remains neutral w.r.t. the validity of the Proposition it evaluates." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <https://example.org/sepio-model/direction> ],
        [ sh:class <https://example.org/sepio-model/RecordMetadata> ;
            sh:description "Provenance metadata about a specific concrete record of information as encoded/serialized in a particular data set or object (as opposed to provenance about the abstract information content the encoding carries)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 22 ;
            sh:path <https://example.org/sepio-model/recordMetadata> ],
        [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "The relationship declared to hold between the subject and the object of the Statement." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path <https://example.org/sepio-model/predicate> ],
        [ sh:datatype xsd:string ;
            sh:description "A document in which the Information Entity is reported." ;
            sh:nodeKind sh:Literal ;
            sh:order 20 ;
            sh:path <https://example.org/sepio-model/reportedIn> ],
        [ sh:class <https://example.org/sepio-model/EvidenceLine> ;
            sh:description "An evidence-based argument that supports or disputes the validity of the proposition that a Statement assesses or puts forth as true. The strength and direction of this argument (whether it supports or disputes  the proposition, and how strongly) is based on an interpretation of one or more pieces of information as evidence (i.e. 'Evidence Items)." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 11 ;
            sh:path <https://example.org/sepio-model/hasEvidenceLines> ],
        [ sh:datatype xsd:string ;
            sh:description "Specific methods that were executed to directly or indirectly support creation of the Information Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path <https://example.org/sepio-model/supportingMethods> ],
        [ sh:datatype xsd:string ;
            sh:description "A term describing a type of evidence used to assess the validity of Statement's proposition (e.g. 'sequence similarity evidence', 'in vitro assay evidence')." ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path <https://example.org/sepio-model/hasEvidenceOfTypes> ],
        [ sh:datatype xsd:string ;
            sh:description "Alternative name(s) for the Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 27 ;
            sh:path <https://example.org/sepio-model/alternativeLabels> ],
        [ sh:datatype xsd:string ;
            sh:description "A natural-language expression of what a Statement asserts to be true." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://example.org/sepio-model/statementText> ],
        [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "Types of methodological approaches that were executed to directly or indirectly support creation of the Information Entity." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 18 ;
            sh:path <https://example.org/sepio-model/supportingMethodTypes> ],
        [ sh:class <https://example.org/sepio-model/InformationEntity> ;
            sh:description "A piece of information that represents or contributes to an argument for or against the validity of the Proposition put forth in a Statement. This is a shortcut relation that links a Statement directly to a piece of evidence supporting it, bypassing the Evidence Line class when used data creators do not utilize an Evidence Line object." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 12 ;
            sh:path <https://example.org/sepio-model/hasEvidence> ],
        [ sh:datatype xsd:string ;
            sh:description "The 'logical' identifier of the entity in the system of record, e.g. a UUID.  This 'id' is unique within a given system, but may or may not be globally unique outside the system. It is used within a system to reference one object from another." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 23 ;
            sh:path <https://example.org/sepio-model/id> ],
        [ sh:datatype xsd:string ;
            sh:description "A primary name for the Entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 26 ;
            sh:path <https://example.org/sepio-model/label> ],
        [ sh:class <https://example.org/sepio-model/Proposition> ;
            sh:description "A possible fact that the Statement assesses or puts forth as true." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://example.org/sepio-model/proposition> ],
        [ sh:datatype xsd:string ;
            sh:description "A specification that describes all or part of the process that led to creation of the Information Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path <https://example.org/sepio-model/specifiedBy> ],
        [ sh:datatype xsd:float ;
            sh:description "A quantitative score that indicates the strength of a Proposition's validity assessment in the direction indicated (i.e. how strongly supported or disputed the Proposition is believed to be).  Depending on its implementation, a score may reflect how *confident* that agent is that the Proposition is true or false, or the *strength of evidence* they believe supports or disputes it." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path <https://example.org/sepio-model/score> ],
        [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "A single term or phrase summarizing the outcome of direction and strength assessments of a Statement's proposition, in terms of a classification of its subject." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 9 ;
            sh:path <https://example.org/sepio-model/subjectClassification> ],
        [ sh:class <https://example.org/sepio-model/Extension> ;
            sh:description "A list of extensions to the Entity, that allow for capture of information not directly supported by elements defined in the model." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 29 ;
            sh:path <https://example.org/sepio-model/extensions> ],
        [ sh:datatype xsd:string ;
            sh:description "An entity or concept in the world that the information entity describes/is about." ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path <https://example.org/sepio-model/isAbout> ],
        [ sh:datatype xsd:string ;
            sh:description "A term used to report the strength of a Proposition's assessment in the direction indicated (i.e. how strongly supported or disputed the Proposition is believed to be).  Implementers may choose to frame a strength assessment in terms of how *confident* an agent is that the Proposition is true or false, or in terms of the *strength of all evidence* they believe supports or disputes it." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path <https://example.org/sepio-model/strength> ],
        [ sh:datatype xsd:string ;
            sh:description "A globally-unique 'business' identifier or accession number for the real-world entity represented by a data object. These are typically assigned by an external system or authority, and used to connect entities and share content across different systems." ;
            sh:nodeKind sh:Literal ;
            sh:order 24 ;
            sh:path <https://example.org/sepio-model/identifiers> ],
        [ sh:datatype xsd:string ;
            sh:description "The Entity or concept about which the Statement is made." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://example.org/sepio-model/subject> ],
        [ sh:datatype xsd:string ;
            sh:description "Indicates when the information content expressed in the Information Entity was generated." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path <https://example.org/sepio-model/dateAuthored> ],
        [ sh:class <https://example.org/sepio-model/Qualifier> ;
            sh:description "An additional piece of information that extends or refines the meaning of a Statement's core subject - predicate - object 'triple' - by providing additional detail, or constraining the statement to apply in a particular context." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path <https://example.org/sepio-model/qualifier> ],
        [ sh:datatype xsd:string ;
            sh:description "An Entity or concept that is related to the subject of a Statement via its predicate." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path <https://example.org/sepio-model/object> ] ;
    sh:targetClass <https://example.org/sepio-model/Statement> .

<https://example.org/sepio-model/Activity> a sh:NodeShape ;
    rdfs:subClassOf <https://example.org/sepio-model/Entity> ;
    sh:closed true ;
    sh:description "An action or set of actions performed by an agent, that occurs over a period of time. Activities may use, generate, modify, move, or destroy one or more entities." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Activity" ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "A primary name for the Entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path <https://example.org/sepio-model/label> ],
        [ sh:class <https://example.org/sepio-model/Entity> ;
            sh:description "An entity that was output from the Activity (could be material or information)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path <https://example.org/sepio-model/output> ],
        [ sh:class <https://example.org/sepio-model/Entity> ;
            sh:description "An entity that was input into the Activity (could be material or information)" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path <https://example.org/sepio-model/input> ],
        [ sh:datatype xsd:string ;
            sh:description "The 'logical' identifier of the entity in the system of record, e.g. a UUID.  This 'id' is unique within a given system, but may or may not be globally unique outside the system. It is used within a system to reference one object from another." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <https://example.org/sepio-model/id> ],
        [ sh:class <https://example.org/sepio-model/Extension> ;
            sh:description "A list of extensions to the Entity, that allow for capture of information not directly supported by elements defined in the model." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 12 ;
            sh:path <https://example.org/sepio-model/extensions> ],
        [ sh:datatype xsd:date ;
            sh:description "The date that the Activity was completed." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://example.org/sepio-model/date> ],
        [ sh:datatype xsd:string ;
            sh:description "The name of the class that is instantiated by a data object representing the Entity." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path <https://example.org/sepio-model/type> ],
        [ sh:datatype xsd:string ;
            sh:description "A free text description of the Entity." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path <https://example.org/sepio-model/description> ],
        [ sh:datatype xsd:string ;
            sh:description "Alternative name(s) for the Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path <https://example.org/sepio-model/alternativeLabels> ],
        [ sh:class <https://example.org/sepio-model/Coding> ;
            sh:description "A specific type of activity the Activity instance represents." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <https://example.org/sepio-model/subtype> ],
        [ sh:class <https://example.org/sepio-model/Agent> ;
            sh:description "An Agent who participated in executing the Activity." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path <https://example.org/sepio-model/performedBy> ],
        [ sh:datatype xsd:string ;
            sh:description "A globally-unique 'business' identifier or accession number for the real-world entity represented by a data object. These are typically assigned by an external system or authority, and used to connect entities and share content across different systems." ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path <https://example.org/sepio-model/identifiers> ],
        [ sh:class <https://example.org/sepio-model/Method> ;
            sh:description "A method that was followed in performing an Activity, that describes how it was executed." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path <https://example.org/sepio-model/specifiedBy> ] ;
    sh:targetClass <https://example.org/sepio-model/Activity> .

<https://example.org/sepio-model/Characteristic> a sh:NodeShape ;
    rdfs:subClassOf <https://example.org/sepio-model/Utility> ;
    sh:closed true ;
    sh:description "An object holding a name-value pair used to describe a trait or role of an individual member of a StudyGroup." ;
    sh:ignoredProperties ( rdf:type ) ;
