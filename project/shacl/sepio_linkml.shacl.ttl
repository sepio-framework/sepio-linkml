            sh:path <https://example.org/sepio-model/valueOperator> ],
        [ sh:datatype xsd:string ;
            sh:description "The specific value(s) that the indicated traitor role  holds in all population members (e.g. 'east asian', 'female', 'adolescent', 'cancer')." ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://example.org/sepio-model/values> ],
        [ sh:datatype xsd:string ;
            sh:description "The type of the trait or role described by the trait (e.g. 'ethnicity', 'sex', 'age', 'disease status')." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://example.org/sepio-model/name> ] ;
    sh:targetClass <https://example.org/sepio-model/Characteristic> .

<https://example.org/sepio-model/DataSet> a sh:NodeShape ;
    rdfs:subClassOf <https://example.org/sepio-model/InformationEntity> ;
    sh:closed true ;
    sh:description "A collection of related data items or records that are organized together in a common format or structure, to enable their computational manipulation as a unit." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Data Set" ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Specific methods that were executed to directly or indirectly support creation of the Information Entity." ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path <https://example.org/sepio-model/supportingMethods> ],
        [ sh:class <https://example.org/sepio-model/Contribution> ;
            sh:description "Specific actions taken by an Agent toward the creation, modification, validation, or deprecation of an Information Entity." ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
