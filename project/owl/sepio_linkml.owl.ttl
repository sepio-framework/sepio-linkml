        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty <https://example.org/sepio-model/systemVersion> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/code> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/label> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty <https://example.org/sepio-model/code> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/systemVersion> ],
        [ a owl:Restriction ;
