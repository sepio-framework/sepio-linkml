        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/values> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/name> ],
        [ a owl:Restriction ;
