            owl:onProperty <https://example.org/sepio-model/date> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://example.org/sepio-model/Method> ;
            owl:onProperty <https://example.org/sepio-model/specifiedBy_2> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://example.org/sepio-model/Coding> ;
            owl:onProperty <https://example.org/sepio-model/subtype_5> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/date> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/subtype_5> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://example.org/sepio-model/Entity> ;
            owl:onProperty <https://example.org/sepio-model/input> ],
