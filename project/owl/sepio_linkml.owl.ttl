            owl:onProperty <https://example.org/sepio-model/pmid> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/pmid> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/license> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/license> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/title> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/pmid> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty <https://example.org/sepio-model/license> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/doi> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/subtype_2> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/title> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/urls_2> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/subtype_2> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
