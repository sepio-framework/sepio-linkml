            owl:onProperty <https://example.org/sepio-model/value> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/systemURL> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://example.org/sepio-model/systemVersion> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/systemVersion> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://example.org/sepio-model/value> ],
        [ a owl:Restriction ;
